<% if(productos.length !== 0 ) { %>
    <div class="table-responsive">
        <table class="table table-dark">
            <tr style="color: yellow;"> <th>Nombre</th> <th>Precio</th> <th>URL de Imagen</th> </tr>
         <% for (const i in productos) { %>       
            <tr>
                <td> <%= productos[i].title %> </td>
                <td> <%= productos[i].price %> </td>
                <td> <img src=<%= productos[i].thumbnail %> style='width:40px; height:40px;'></td>
            </tr>     
         <% } %>      
        </table>
    </div>
<% } else { %>
    <h3 style="color: black">No se encontraron datos</h3>
<% } %>

